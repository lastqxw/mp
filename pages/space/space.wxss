/* pages/space/space.wxss */
.item {
  margin: 18rpx 32rpx;
  background: #ffffff;
  border: 1px solid #eaeaef;
  opacity: 1;
  border-radius: 4px;
}
.item image {
  width: 100%;
  height: 160px;
  background: rgba(0, 0, 0, 0);
  opacity: 1;
  border-radius: 4px 4px 0px 0px;
}
.item-des {
  padding: 20rpx 24rpx;
}
.item-des-top {
  display: flex;
  justify-self: self-start;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.level {
  border: 1px solid #ff8104;
  opacity: 1;
  border-radius: 4px;
  padding: 0rpx 14rpx;
  font-size: 13px;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  color: #ff8104;
  opacity: 1;
  margin-right: 16rpx;
}
.name {
  font-size: 15px;
  font-family: Alibaba PuHuiTi;
  font-weight: bold;
  line-height: 21px;
  color: #000000;
  opacity: 0.9;
}
.item-des-top-right text {
  padding: 6rpx 36rpx;
  background: rgba(56, 198, 112, 0.2);
  border-radius: 16px;
  font-size: 12px;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 17px;
  color: #38c670;
  opacity: 1;
}
.item-des-bottom {
  font-size: 14px;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 20px;
  color: #000000;
  opacity: 0.5;
}
