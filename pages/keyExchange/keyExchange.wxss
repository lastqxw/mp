page{
  background: #F6F7F9;
}
.key_swiper{
  position: relative;
  width: 100%;
}
.swiper-item{
  position: relative;
  width: 100%;
  height: 100%;
}
.swiper-item image{
  width: 100%;
}
.dots{
  position: absolute;
  right: 24rpx;
  bottom: 24rpx;
  padding: 8rpx 20rpx;
  font-size: 24rpx;
  color: #fff;
  background: rgba(0,0,0,0.7);
  border-radius: 48rpx;
}
.key_title{
  padding: 24rpx 32rpx;
  background: #0386FC;
  color: #fff;
}
.key_name{
  font-size: 32rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 500;
  line-height: 40rpx;
}
.key_des{
  margin-top: 10rpx;
  font-size: 24rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 32rpx;
  color: rgba(255,255,255,0.9);
}
.key_con{
  padding:30rpx 32rpx 0 32rpx;
}
.key_parameter{
  padding:20rpx 20rpx 30rpx 20rpx;
  background-color: #fff;
  border-radius: 18rpx;
}
.key_parameter_top{
  position: relative;
  padding-bottom: 30rpx;
  font-size: 30rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 42rpx;
  color: #000000;
  opacity: 0.9;
}
.key_parameter_top image{
  position: absolute;
  top: 0rpx;
  right: 0;
  width: 26rpx;
  height: 26rpx;
}
.key_parameter_bot{
  font-size: 30rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 42rpx;
  color: #000000;
  opacity: 0.9;
}
.key_bot {
  margin-top:20rpx;
  padding:20rpx 20rpx 30rpx 20rpx;
  background-color: #fff;
  border-radius: 18rpx;
  font-size: 30rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 42rpx;
  color: #000000;
  opacity: 0.9;
}
.key_bot image{
  width: 100%;
  padding-top: 20rpx;
}
.key_exchange{
  padding: 36rpx 32rpx 24rpx 32rpx;
}
button{
  background-color: #FF8104!important;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 34rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 500;
}
.ischeck{
  width: 100%;
  padding-top: 40rpx;
}
.van-checkbox{
  justify-content: center;
  font-size: 28rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 40rpx;
  color: #0AC956;
  opacity: 1;
}
van-icon{
  font-size:32rpx!important;
}
.statement{
  display: inline-block;
  color:#0AC956;
}
.title{
  font-size: 14px;
  text-align: center;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
  color: #000000;
}
.sub{
  font-size: 12px;
  margin-bottom: 20rpx;
  color: #000000;
  opacity: 0.9;
  padding:0px 32rpx
}
.con{
  font-size: 12px;
  margin-bottom: 20rpx;
  color: #000000;
  opacity: 0.9;
  padding:0px 32rpx
}